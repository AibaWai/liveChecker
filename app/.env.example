# Instagram Configuration
IG_USERNAME=target_username_here

# Instagram Cookies (JSON format array)
# Get these from your browser after logging into Instagram
# Example: [{"name":"sessionid","value":"your_session_id","domain":".instagram.com","path":"/","expires":1234567890,"httpOnly":true,"secure":true}]
IG_COOKIES=[]

# Discord Configuration
# Create a webhook in your Discord channel: Server Settings > Integrations > Webhooks
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/your_webhook_url_here

# Optional: Discord Channel ID (for reference)
DISCORD_CHANNEL_ID=your_channel_id_here

# Monitoring Configuration
# Check interval in milliseconds (60000 = 1 minute)
CHECK_INTERVAL=60000

# Koyeb Port (usually set automatically by Koyeb)
PORT=3000